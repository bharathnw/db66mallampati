extends layout 
 
block content 
  if (!user) 
    a(href="/login") Login 
    br 
    a(href="/register") Register 
  if (user) 
    p You are currently logged in as #{user.username} 
    a(href="/car") Take me to cars page 
    br 
    a(href="/logout") Logout 


  script.
    let client_count = 0;
    function update(){
      client_count++;
      document.getElementById('target').innerHTML = `Value now ${client_count}`;
    }
